{
  "extends": [
    "airbnb",
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".tsx"
        ]
      }
    ],
    "react/function-component-definition": [
      2,
      {
        "namedComponents": "arrow-function"
      }
    ],
    "import/extensions": 0,
    "import/prefer-default-export": 0
  }
}
